      "source": [
        "import math\n",
        "\n",
        "x = 25\n",
        "akar_kuadrat = math.sqrt(x)\n",
        "print(f\"Akar kuadrat dari {x} adalah: {akar_kuadrat}\")\n",
        "\n",
        "n = 7\n",
        "faktorial = math.factorial(n)\n",
        "print(f\"Faktorial dari {n} adalah: {faktorial}\")\n",
        "\n",
        "base = 3\n",
        "exponent = 5\n",
        "pangkat = math.pow(base, exponent)\n",
        "print(f\"{base} pangkat {exponent} adalah: {pangkat}\")\n",
        "\n",
        "sudut_radian = math.radians(45)\n",
        "cosinus = math.cos(sudut_radian)\n",
        "print(f\"Cosinus dari 45 derajat adalah: {cosinus}\")\n",
        "\n",
        "nilai = 256\n",
        "basis = 2\n",
        "logaritma = math.log(nilai, basis)\n",
        "print(f\"Logaritma dari {nilai} dengan basis {basis} adalah: {logaritma}\")\n"
      ],
